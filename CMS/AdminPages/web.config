<?xml version="1.0"?>
<configuration>

  <location path="Register.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

  <system.web>
    <authorization>
      <deny users="?" />
      <allow roles="Admin" />
      <deny roles="General" />
    </authorization>
  </system.web>

</configuration>
